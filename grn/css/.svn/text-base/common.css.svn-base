    /*重设导航栏样式*/
.header .navbar{
    background-color: #4eb9e3;;
    border: none;
}
a.navbar-brand{
    padding: 4px 15px;
    text-align: center;
}
.header .dropdown-menu{
    background-color: #000;
    opacity: 0.5;
    border: none;
}
.navbar a,.navbar-default .navbar-nav>li>a{
    color: #fff;
}
.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:focus, .navbar-default .navbar-nav>.open>a:hover{
    color: #fff;
    opacity: 0.7;
    background-color: inherit;
}
.navbar-default .navbar-nav>li>a:focus, .navbar-default .navbar-nav>li>a:hover{
    color: #fff;
}
.header .nav>li>a span{
    display: none;
    position: absolute;
    bottom: 0;
    height: 3px;
    width: 32px;
    left: 50%;
    transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
    -ms-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
    background-color: #fff;
}
#bs-example-navbar-collapse-1 a[aria-expanded="false"]+ul{
    opacity: 0;
    transition: opacity 0.3s;
}
.navbar-default .navbar-toggle:focus, .navbar-default .navbar-toggle:hover{
    background-color:transparent;
}
.navbar-default .navbar-toggle{
    border-color:transparent;
}
.navbar-default .navbar-toggle .icon-bar{
    background-color: #fff;
}
#bs-example-navbar-collapse-1 a{
    font-size: 13px;
}
    /*x的样式*/
.header .menu {
    font-family: arial;
    box-sizing: border-box;
    position: relative;
    -moz-transition: 0.3s;
    -o-transition: 0.3s;
    -webkit-transition: 0.3s;
    transition: 0.3s;
}
.header .bar {
    box-sizing: border-box;
    position: relative;
    background-color: #000;
    -moz-transition: 0.4s;
    -o-transition: 0.4s;
    -webkit-transition: 0.4s;
    transition: 0.4s;
}
.header .menu {
    overflow: hidden;
}
.header .menu:after {
    box-sizing: border-box;
    content: '';
    width: 90%;
    height: 90%;
    left: 0;
    top: 0;
    position: absolute;
}
.header .menu.hover:after {
    -moz-animation: circle-creation 1s forwards;
    -webkit-animation: circle-creation 1s forwards;
    animation: circle-creation 1s forwards;
}
.header .menu.hover .bar:nth-of-type(1) {
    -moz-transform: translateY(6px) rotate(45deg);
    -ms-transform: translateY(6px) rotate(45deg);
    -webkit-transform: translateY(6px) rotate(45deg);
    transform: translateY(6px) rotate(45deg);
}
.header .menu.hover .bar:nth-of-type(2) {
    -moz-transform: translateX(100px);
    -ms-transform: translateX(100px);
    -webkit-transform: translateX(100px);
    transform: translateX(100px);
    -moz-transition: 0.6s;
    -o-transition: 0.6s;
    -webkit-transition: 0.6s;
    transition: 0.6s;
}
.header .menu.hover .bar:nth-of-type(3) {
    -moz-transform: translateY(-6px) rotate(-45deg);
    -ms-transform: translateY(-6px) rotate(-45deg);
    -webkit-transform: translateY(-6px) rotate(-45deg);
    transform: translateY(-6px) rotate(-45deg);
}
.nav-detial-HMZ{
    font-size: 10px;
}
div.nav-group-HMZ{
    display: inline-block;
    overflow: hidden;
}
div.nav-group-HMZ a{
    float: left;
    display: inline-block;
    padding: 11px 25px;
    position: relative;
    height:90px;
    min-width: 55px;
    text-align: center;
    box-sizing: border-box;
    opacity: 0.6;
    transition: opacity 0.2s;
    -webkit-transition: opacity 0.2s;
    -ms-transition: opacity 0.2s;
    -moz-transition: opacity 0.2s;
}
@media (max-width: 768px){
    div.nav-group-HMZ a{
        opacity: 1;
    }
}
@media (min-width: 992px){
    div.nav-group-HMZ a{
